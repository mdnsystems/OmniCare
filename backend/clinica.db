    true,
    '2024-01-15 08:00:00',
    NOW(),
    NOW()
FROM usuarios u, mensagens m
WHERE u.tenant_id = 'tenant-001' 
  AND m.tenant_id = 'tenant-001'
  AND u.role = 'ADMIN'
LIMIT 1;

