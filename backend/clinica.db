SELECT 
    'DADOS INSERIDOS COM SUCESSO!' as status,
    COUNT(*) as total_clinicas,
    (SELECT COUNT(*) FROM usuarios WHERE tenant_id = 'tenant-001') as total_usuarios,
    (SELECT COUNT(*) FROM pacientes WHERE tenant_id = 'tenant-001') as total_pacientes,
    (SELECT COUNT(*) FROM agendamentos WHERE tenant_id = 'tenant-001') as total_agendamentos,
    (SELECT COUNT(*) FROM faturamentos WHERE tenant_id = 'tenant-001') as total_faturamentos,
    (SELECT COUNT(*) FROM chats WHERE tenant_id = 'tenant-001') as total_chats
FROM clinicas WHERE tenant_id = 'tenant-001';

