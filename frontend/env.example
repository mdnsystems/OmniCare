# =============================================================================
# CONFIGURAÇÃO DE AMBIENTE - OMNICARE FRONTEND
# =============================================================================
# 
# Copie este arquivo para .env e configure as variáveis conforme necessário
# 
# =============================================================================

# URL da API Backend
VITE_API_URL=http://localhost:8080/api

# Configurações de Desenvolvimento
VITE_DEV_MODE=true
VITE_ENABLE_LOGS=false

# Configurações de WebSocket
VITE_WS_URL=ws://localhost:8080

# Configurações de CEP (opcional)
VITE_CEP_API_URL=https://viacep.com.br/ws

# Configurações de Analytics (opcional)
VITE_ANALYTICS_ID=

# Configurações de Sentry (opcional)
VITE_SENTRY_DSN=

# Configurações de Feature Flags
VITE_ENABLE_WHATSAPP=true
VITE_ENABLE_FINANCIAL=true
VITE_ENABLE_REPORTS=true

# Configurações de Segurança
VITE_ENABLE_DEBUG_LOGS=false
VITE_ENABLE_SENSITIVE_LOGS=false

# Configurações de Z-API (WhatsApp) - opcional
VITE_ZAPI_TOKEN=
VITE_ZAPI_URL=https://api.z-api.io 